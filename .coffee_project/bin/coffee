#!/home/fernando/Dropbox/Programação/Python/Coffee_Project/Coffee-app-api/.coffee_project/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'coffee==0.1.0','console_scripts','coffee'
__requires__ = 'coffee==0.1.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('coffee==0.1.0', 'console_scripts', 'coffee')()
    )
